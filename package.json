{
    "name": "react-boilerplate",
    "version": "0.0.0",
    "description": "The React starter kit for an Express application",
    "scripts": {
        "prebuild": "npm-run-all clean-dist build:html",
        "build": "babel-node util/build.js",
        "build:html": "babel-node util/build-html.js",
        "postbuild": "babel-node src/server/server.prod.js",
        "clean-dist": "node_modules/.bin/rimraf ./src/client/dist && mkdir src/client/dist",
        "dev": "babel-node src/server/server.dev.js",
        "lint": "eslint src"
    },
    "author": "Philippe Mimms",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "autoprefixer": "^7.0.1",
        "babel-cli": "^6.24.1",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "body-parser": "^1.17.1",
        "cheerio": "^0.22.0",
        "compression": "^1.6.2",
        "css-loader": "^0.28.1",
        "eslint": "^3.19.0",
        "eslint-plugin-babel": "^4.1.1",
        "eslint-plugin-react": "^7.0.0",
        "express": "^4.15.2",
        "extract-text-webpack-plugin": "^2.1.0",
        "node-sass": "^4.5.2",
        "npm-run-all": "^4.0.2",
        "open": "^0.0.5",
        "postcss-loader": "^1.3.3",
        "precss": "^1.4.0",
        "prop-types": "^15.5.8",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-redux": "^5.0.4",
        "redux": "^3.6.0",
        "redux-thunk": "^2.2.0",
        "rimraf": "^2.6.1",
        "sass-loader": "^6.0.3",
        "style-loader": "^0.17.0",
        "webpack": "^2.4.1"
    },
    "devDependencies": {
        "react-hot-loader": "next",
        "redux-immutable-state-invariant": "^2.0.0",
        "webpack-dev-middleware": "^1.10.2",
        "webpack-hot-middleware": "^2.18.0"
    }
}
